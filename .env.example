LINKEDIN_PASSWORD=
# Personal Profile (stored in JSON)
PROFILE_JSON_PATH=
RESUME_PDF_PATH=

# Task Settings
APPLY_NUMBER=

# CDP (optional, connect to an already running Chrome)
CDP_URL=

# Profile copy (optional)
# If true, copy CHROME_USER_DATA_DIR + CHROME_PROFILE_DIR to a temp dir each run.
COPY_NON_DEFAULT_PROFILE=true

# LLM Settings (default keeps current behavior)
LLM_PROVIDER=google
LLM_MODEL=gemini-3-flash-preview

# Chrome profile (optional; set to use your existing logged-in Chrome)
# Windows example:
# CHROME_EXECUTABLE_PATH=C:/Program Files/Google/Chrome/Application/chrome.exe
# CHROME_USER_DATA_DIR=%LOCALAPPDATA%/Google/Chrome/User Data
# macOS example:
# CHROME_EXECUTABLE_PATH=/Applications/Google Chrome.app/Contents/MacOS/Google Chrome
# CHROME_USER_DATA_DIR=~/Library/Application Support/Google/Chrome
# Linux example:
# CHROME_EXECUTABLE_PATH=/usr/bin/google-chrome
# CHROME_USER_DATA_DIR=~/.config/google-chrome
CHROME_PROFILE_DIR=Default
CHROME_EXECUTABLE_PATH=
CHROME_USER_DATA_DIR=

# Browser Use Cloud (optional)
BROWSER_USE_API_KEY=

# Google Gemini
GOOGLE_API_KEY=

# Disable telemetry (optional)
ANONYMIZED_TELEMETRY=false
